   "source": [
    "inputlog = input('enter the input log: ').split(' ')\n",
    "print(inputlog)\n",
    "s=0\n",
    "for i in inputlog:\n",
    "    \n",
    "    if 'D' in i:\n",
    "        d = int(i.split(':')[-1])\n",
    "        s=s+d\n",
    "        \n",
    "    elif 'W' in i:\n",
    "        w = int(i.split(':')[-1])\n",
    "        s = s-w\n",
    "print(s)"
   ]
